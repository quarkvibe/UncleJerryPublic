# Development Environment Variables for Uncle Jerry Blueprint Analyzer

# API URL Configuration
# This is the location of your backend API server
REACT_APP_API_URL=http://localhost:3001

# Feature Flags
REACT_APP_ENABLE_CLAUDE_INTEGRATION=true
REACT_APP_ENABLE_MOCK_API=true
REACT_APP_ENABLE_DEBUG_LOGS=true

# Content Settings
REACT_APP_MAX_UPLOAD_SIZE_MB=15
REACT_APP_SUPPORTED_FILE_TYPES=image/jpeg,image/png,image/tiff,application/pdf

# Cache and Performance
REACT_APP_API_TIMEOUT_MS=30000
REACT_APP_CACHE_DURATION_MIN=15

# Application Information
REACT_APP_VERSION=$npm_package_version
REACT_APP_BUILD_DATE=$(date -u +'%Y-%m-%dT%H:%M:%SZ')

# Demo Settings
REACT_APP_DEMO_MODE=true
REACT_APP_DEMO_USER_ID=user123